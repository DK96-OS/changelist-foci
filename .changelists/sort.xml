<sorting>
    <changelist name="CL-FOCI Input Package">
        <files path_start="changelist_foci/input" />
    </changelist>
    <changelist name="CL-FOCI Main Package">
        <files path_start="changelist_foci/" />
    </changelist>
    <changelist name="Test Input Package">
        <files path_start="test/input" />
    </changelist>
    <changelist name="Test Main Package">
        <files path_start="test/" />
    </changelist>
    <changelist name="Documentation">
        <files file_ext="md" />
    </changelist>
    <changelist name="GitHub Workflows">
        <files path_start=".github/workflows" />
        <files file_ext="yml" />
    </changelist>
    <changelist name="GitHub Dependabot">
        <files path_start=".github/" />
        <files filename_prefix="dependabot" />
        <files file_ext="yml" />
    </changelist>
    <changelist key="root_python" name="Root Project Python Files" module="root">
        <files file_ext="py"/>
    </changelist>
    <changelist key="hidden_files" name="Hidden Files In Hidden Directories" module="hidden">
        <files filename_prefix="."/>
    </changelist>
    <changelist key="root" name="Hidden Root Project Files" module="root">
        <files filename_prefix="."/>
    </changelist>
</sorting>